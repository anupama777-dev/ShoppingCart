<section>
    <div class="container">
        <table class="table table-bordered mt-5">
            <thead>
                <tr>
                    <th scope="col">UserId</th>
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                </tr>
            </thead>
            <tbody>
                {{#each users}}
                <tr>
                    <td>{{this._id}}</td>
                    <td>{{this.Name}}</td>
                    <td>{{this.Email}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</section>